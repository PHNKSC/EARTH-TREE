-- Scripts:

local function QORZ_fake_script() -- UIGradient.gdnt rgb 
	local script = Instance.new('LocalScript', UIGradient)

	local uigd = script.Parent
	while true do
		uigd.Rotation += 0.5
		wait()
	end
end
coroutine.wrap(QORZ_fake_script)()
local function YDGJ_fake_script() -- UIGradient_2.gdnt rgb 
