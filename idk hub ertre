end
coroutine.wrap(TBAJSLB_fake_script)()
local function MZLP_fake_script() -- chooses.bguito 
	local script = Instance.new('LocalScript', chooses)

	local uits = script.Parent.Parent.chooses
	
	_G.togt = true
	while _G.togt == true do
		uits.Transparency += -0.1
	wait()
	end
end
coroutine.wrap(MZLP_fake_script)()
local function BVDHOKR_fake_script() -- startui.nof 
	local script = Instance.new('LocalScript', startui)

	local noff = script["Audio/notify-ask"]
	
	noff:Play()
	
	
end
coroutine.wrap(BVDHOKR_fake_script)()
