coroutine.wrap(FCYCUAX_fake_script)()
local function HIGUXGT_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local strt = script["Windows 11 Calendar Notify"]
	local thisbtnstr = script.Parent
	local HIDE = script.Parent.Parent.Parent.Parent.bgb
	
	thisbtnstr.MouseButton1Click:Connect(function()
		strt.Volume = 50
		strt:Play()
		HIDE.Visible = false
	end)
	
end
coroutine.wrap(HIGUXGT_fake_script)()
local function TBAJSLB_fake_script() -- UIGradient_5.gdnt rgb 
